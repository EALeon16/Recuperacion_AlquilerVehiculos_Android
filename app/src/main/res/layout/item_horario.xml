<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="205dp"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textViegw2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Fecha Recogida: "
            android:textSize="14sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lblfRecogida"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView" />

        <TextView
            android:id="@+id/textViehw6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Fecha Devolucion: "
            android:textSize="14sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lblfDevolucion"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView" />
        <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Dias: "
            android:textSize="14sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lblDias"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Precio Total: "
            android:textSize="14sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lblPrecio"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView" />
        <TextView
            android:id="@+id/textVeriew6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Nro Registro: "
            android:textSize="14sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lblnro"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView" />

        <View
            android:id="@+id/divider6"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="?android:attr/listDivider" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="205dp"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Placa: "
            android:textSize="14sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lblplaca"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView" />
        <TextView
            android:id="@+id/textVdfiew3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Marca: "
            android:textSize="14sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lblMarca"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView" />
        <TextView
            android:id="@+id/textViewcas3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Modelo: "
            android:textSize="14sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lblModelo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView" />
        <TextView
            android:id="@+id/textViasdew3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="tipo Vehiculo: "
            android:textSize="14sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lblTipoVehiculo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView" />



        <View
            android:id="@+id/divider5"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_weight="1"
            android:background="?android:attr/listDivider" />
    </LinearLayout>

</LinearLayout>